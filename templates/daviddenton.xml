            <option name="JAVA_CODE" value="true" />
        </context>
    </template>
    <template name="before" value="@org.junit.jupiter.api.BeforeEach&#10;fun $name$() {&#10;    $content$&#10;}" description="" toReformat="true" toShortenFQNames="true">
